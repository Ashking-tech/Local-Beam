{
  "name": "sockets",
  "version": "1.0.0",
  "main": "electron.js",
  "scripts": {
    "start": "electron .",
    "dist": "electron-builder"
  },

  "build": {
    "appId": "com.ash.lanshare",
    "productName": "LAN Share",
    "directories": {
      "output": "dist"
    },
    "files": [
      "**/*",
      "!node_modules/.cache/**/*"
    ],
    "extraResources": [
      "uploads"
    ],
    "linux": {
      "target": ["deb", "AppImage"],
      "category": "Utility"
    },
    "win": {
      "target": "nsis"
    }
  },

  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "dgram": "^1.0.1",
    "ejs": "^3.1.10",
    "express": "^5.1.0",
    "multer": "^2.0.2",
    "nodemon": "^3.1.10"
  },
  "devDependencies": {
    "electron": "^37.2.5",
    "electron-builder": "^26.0.12"
  }
}
